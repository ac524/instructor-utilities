{
  "name": "crsm",
  "version": "0.7.2",
  "description": "Classroom student management utilities for instructors to effectively organize and keep track of student progress with a team of staff members.",
  "main": "app/server.js",
  "_moduleAliases": {
    "~crsm": "app",
    "~crsmmodels": "app/controllers/definitions/models",
    "~crsmtest": "test"
  },
  "scripts": {
    "envsetup": "node envsetup",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node app/server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node seed",
    "seed:reset": "node app/seed/reset",
    "install": "cd client && npm install",
    "postinstall": "link-module-alias",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "node test/run"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "concurrently": "7.0.0",
    "inquirer": "8.2.0",
    "link-module-alias": "1.2.0",
    "mocha": "9.2.0",
    "nodemon": "2.0.15",
    "nyc": "15.1.0",
    "sinon": "13.0.1",
    "sinon-chai": "3.7.0"
  },
  "dependencies": {
    "@sendgrid/mail": "7.6.1",
    "apollo-server-core": "3.6.3",
    "apollo-server-express": "3.6.3",
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "fastest-validator": "1.12.0",
    "graphql": "15.8.0",
    "graphql-modules": "2.0.0",
    "graphql-type-json": "0.3.2",
    "if-env": "1.0.4",
    "jsonwebtoken": "8.5.1",
    "mjml": "4.12.0",
    "mongoose": "5.13.14",
    "nodemailer": "6.7.2",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "socket.io": "4.4.1"
  }
}
